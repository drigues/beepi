<h1>Edit Directory Profile</h1>
<%= form_with model: @directory, local: true do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>
  <%= f.submit "Save" %>
<% end %>
<%= link_to "Back to Dashboard", dashboard_path %>