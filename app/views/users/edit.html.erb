<h1>Edit Profile</h1>
<%= form_with(model: @user, url: profile_path, method: :patch) do |f| %>
  <p>
    <%= f.label :directory %><br>
    <%= f.text_area :directory %>
  </p>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>
  <%= f.submit "Save" %>
<% end %>
<%= link_to "Back to Dashboard", dashboard_path %>